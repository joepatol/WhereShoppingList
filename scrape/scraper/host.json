{
    "version": "2.0",
    "logging": {
      "logLevel": {
        "default": "Trace"
      },
      "applicationInsights": {
        "samplingSettings": {
          "isEnabled": true,
          "excludedTypes": "Request"
        }
      }
    },
    "extensions": {
      "http": {
        "routePrefix": ""
      }
    },
    "extensionBundle": {
      "id": "Microsoft.Azure.Functions.ExtensionBundle",
      "version": "[4.0.0, 5.0.0)"
    },
    "customHandler": {
      "description": {
        "defaultExecutablePath": "target/release/scraper",
        "workingDirectory": "",
        "arguments": []
      },
      "enableForwardingHttpRequest": true
    }
  }